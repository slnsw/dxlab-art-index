{
  "name": "dxlab-art-index",
  "version": 1,
  "scope": "dxlab",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/art-index/_next/(.*)",
      "destination": "/_next/$1"
    }
  ]
}
